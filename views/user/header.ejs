                                        <span>Total</span>
                                        <% var total=0;%>
                                        <% var sub=0;%>
                                        <% for(var tp = 0;tp< cartData.length ;tp++){%>
                                          <%  sub =sub + cartData[tp].quantity*cartData[tp].productId.product_price;%>

                                            <%}%>
                                        <span class="cart-total-price"><%=sub%></span>
                                    </div><!-- End .dropdown-cart-total -->
    
                                    <div class="dropdown-cart-action">
                                        <a href="/viewcart" type="button"  class="btn btn-primary">View Cart</a>
                                        <a href="javascript:void(0)" class="btn btn-outline-primary-2"><span>Checkout</span><i class="icon-long-arrow-right"></i></a>
