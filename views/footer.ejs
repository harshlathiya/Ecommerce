  <script>
    function getsubcat(){
      var catid = document.getElementById('product-category').value;
      //alert(catid)
      $.ajax({
        url:"/admin/ecate/getsubcat",
        method:"post",
        data:{
          'catid':catid
        },
        success:function(res){
          $('#product-subcategory').html(res)
        }
      })
    }

    function getecat(){
      var subcatid = document.getElementById('product-subcategory').value;
      //alert(catid)
      $.ajax({
        url:"/admin/brand/getecatadata",
        method:"post",
        data:{
          'subcatid':subcatid
        },
        success:function(res){
          $('#product-ecategory').html(res)
        }
      })
    }
    
    function getbrand(){
      var ecatid = document.getElementById('product-ecategory').value;
      //alert(catid)
      $.ajax({
        url:"/admin/type/getbrands",
        method:"post",
        data:{
          'ecatid':ecatid
        },
        success:function(res){
          $('#product-brand').html(res)
        }
      })
    }


    function gettypes(){
      var brandid = document.getElementById('product-brand').value;
      //alert(catid)
      $.ajax({
        url:"/admin/brand/gettype",
        method:"post",
        data:{
          'brandid':brandid
        },
        success:function(res){
          $('#product-type').html(res)
        }
      })
    }

